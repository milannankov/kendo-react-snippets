{
    "name": "kendo-react-snippets",
    "displayName": "Kendo React Snippets",
    "description": "Visual Studio Code Snippets for Kendo UI for React",
    "version": "0.0.1",
    "publisher": "milannankov",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/javascript.json"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/javascriptreact.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/typescriptreact.json"
            }
        ]
    },
    "devDependencies": {
        "gulp": "^3.9.1",
        "gulp-sequence": "^1.0.0",
        "through2": "^2.0.3"
    },
    "dependencies": {
        "plugin-error": "^1.0.1"
    }
}
